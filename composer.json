{
    "name": "axy/docker-compose-config",
    "type": "library",
    "description": "Work with docker-compose config",
    "keywords": [],
    "homepage": "https://github.com/axypro/docker-compose-config",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Grigoriev",
            "email": "go.vasac@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "axy\\docker\\compose\\config\\": "src",
            "axy\\docker\\compose\\config\\tests\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            "/tests/*",
            "/.github/*",
            "/doc/*",
            "/*.sh",
            "/*.xml.dist",
            "/CHANGELOG.md"
        ]
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist"
    }
}
